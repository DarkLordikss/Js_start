function compareNumbers(a, b) {
  return b - a;
}

var fruit = ['арбузы', 'бананы', 'Вишня'];
fruit.sort();

var numbers = [1, 2, 10, 21, 12, 32, '4', 'да'];
numbers.sort(compareNumbers);

var things = ['слово', 'Слово', '1 Слово', '2 Слова'];
things.sort();

console.log(things);
console.log(numbers);
console.log(fruit);

//Удобная штуку это js ^^
